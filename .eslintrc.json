{
  "$schema": "http://json.schemastore.org/eslintrc",
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "settings": {},
  "plugins": [
    "unused-imports",
    "@typescript-eslint"
  ],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "no-unused-vars": "off",
    "no-undef": "off",
    "@typescript-eslint/no-unused-vars": "warn",
    "semi": "warn",
    "no-debugger": 0,
    "no-console": 0,
    "no-global-assign": 0,
    "unused-imports/no-unused-imports": "warn"
  }
}